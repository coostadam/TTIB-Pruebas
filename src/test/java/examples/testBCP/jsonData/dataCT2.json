{
  "CT2": {
    "debtorParticipantCode": "#regex 0[0-9]{3}",
    "creditorParticipantCode": "#regex 0[0-9]{3}",
    "creationDate": "#regex \\d{8}",
    "creationTime": "#regex \\d{6}",
    "terminalId": "#string? _.length == 8",
    "retrievalReferenteNumber": "#string? _.length == 12",
    "trace": "#number? _.toString().length == 6",
    "channel": "#number? _ == 15 || _ == 91 || _ == 52",
    "amount": "#number",
    "currency": "#number? _ == 604 || _ == 840",
    "transactionReference": "##string",
    "referenceTransactionId": "#string",
    "transactionType": "#number? _ == 320 || _ == 325",
    "feeAmount": "#number",
    "feeCode": "#string",
    "applicationCriteria": "#string",
    "debtorTypeOfPerson": "#string",
    "debtorName": "#string",
    "debtorAddressLine": "##string",
    "debtorIdCode": "#number? _ >= 0 && _ <= 9",
    "debtorId": "#string? _ == '1' || _ == '2' || _ == '3' || _ ==  '4' || _ == '5' || _ == '6'",
    "debtorPhoneNumber": "##string",
    "debtorMobileNumber": "#string",
    "debtorCCI": "#string",
    "creditorName": "##string",
    "creditorAddressLine": "##string",
    "creditorPhoneNumber": "##string",
    "creditorMobileNumber": "##string",
    "creditorCCI": "##string",
    "creditorCreditCard": "##string",
    "sameCustomerFlag": "##string? _ == 'M' || _ == 'O'",
    "purposeCode": "#string",
    "unstructuredInformation": "##string",
    "grossSalaryAmount": "##string",
    "salaryPaymentIndicator": "##string",
    "monthOfThePayment": "##string",
    "yearOfThePayment": "##string",
    "branchId": "#string? _.length <= 35",
    "settlementDate": "#string",
    "instructionId": "#string? _.length <= 35",
    "interbankSettlementAmount": "#number"
  }
}