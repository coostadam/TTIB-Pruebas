{
    "debtorParticipantCode": "#regex 0[0-9]{3}",
    "creditorParticipantCode": "#regex 0[0-9]{3}",
    "creationDate": "#regex \\d{8}",
    "creationTime": "#regex \\d{6}",
    "terminalId": "#string? _.length == 8",
    "retrievalReferenteNumber": "#string? _.length == 12",
    "trace": "#string? _.length == 6",
    "channel": "#string? _ == '15' || _ == '91' || _ == '52'",
    "amount": "#number",
    "currency": "#string? _ == '604' || _ == '840'",
    "transactionReference": "##string? _.length <= 35",
    "referenceTransactionId": "#string? _.length <= 35",
    "transactionType": "#string? _ == '320' || _ == '325'",
    "feeAmount": "#number",
    "feeCode": "#string? _.length == 4",
    "applicationCriteria": "#string? _ == 'M' || _ == 'O' || _ == 'E'",
    "debtorTypeOfPerson": "#string? _ == 'N' || _ == 'J'",
    "debtorName": "#string? _.length <= 140",
    "debtorAddressLine": "##string? _.length <= 70",
    "debtorIdCode": "#string? _ == '1' || _ == '2' || _ == '3' || _ ==  '4' || _ == '5' || _ == '6'",
    "debtorId": "#string? _.length <= 12",
    "debtorPhoneNumber": "##string? _.length == 7",
    "debtorMobileNumber": "##string? _.length == 9",
    "debtorCCI": "#string? _.length == 20",
    "creditorName": "##string? _.length <= 140",
    "creditorAddressLine": "##string? _.length <= 70",
    "creditorPhoneNumber": "##string? _.length == 7",
    "creditorMobileNumber": "##string? _.length == 9",
    "creditorCCI": "##string? _.length == 20",
    "creditorCreditCard": "##string? _.length == 20",
    "sameCustomerFlag": "##string? _ == 'M' || _ == 'O'",
    "purposeCode": "#string? _.length == 4",
    "unstructuredInformation": "##string? _.length <= 140",
    "grossSalaryAmount": "##number",
    "salaryPaymentIndicator": "##string? _ == '4' || _ == '5' || _ == '8' || _ ==  '9'",
    "monthOfThePayment": "##string? _.length == 2",
    "yearOfThePayment": "##string? _.length == 4",
    "branchId": "#string? _.length <= 35",
    "settlementDate": "#string? _.length == 8",
    "instructionId": "#string? _.length <= 35",
    "interbankSettlementAmount": "#number"
}